<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3">
          <i class="fas fa-user-plus"></i>Batik item Register</h1>
          <%- include ("./partials/messages"); %>

        <form action="/item/aaa" method="POST">
            <div class="form-group">
                <label for="itemCode">Item Code</label>
                <input
                  type="name"
                  id="itemCode"
                  name="itemCode"
                  class="form-control"
                  placeholder="Enter itemCode"
                  value="<%= typeof name != 'undefined' ? itemCode : '' %>"
                />

                <div class="form-group">
                  <label for="UserRegNo">UserRegNo</label>
                  <input
                    type="name"
                    id="UserRegNo"
                    name="UserRegNo"
                    class="form-control"
                    placeholder="Enter UserRegNo"
                    value="<%= typeof name != 'undefined' ? UserRegNo : '' %>"
                  />
                </div>

          <div class="form-group">
            <label for="name">Name</label>
            <input
              type="name"
              id="itemName"
              name="itemName"
              class="form-control"
              placeholder="Enter item Name"
              value="<%= typeof name != 'undefined' ? itemName : '' %>"
            />
          </div>
          <div class="form-group">
            <label for="itemPrice">item Price</label>
            <input
              type="name"
              id="itemPrice"
              name="itemPrice"
              class="form-control"
              placeholder="Enter item Price"
              value="<%= typeof email != 'undefined' ? itemPrice : '' %>"
            />
          </div>
          <div class="form-group">
            <label for="itemDescription">Description</label>
            <input
              type="name"
              id="itemDescription"
              name="itemDescription"
              class="form-control"
              placeholder="item Description"
              value="<%= typeof password != 'undefined' ? itemDescription : '' %>"
            />
          </div>
          <div class="form-group">
            <label for="imgUrl">Confirm imgUrl</label>
            <input
              type="name"
              id="imgUrl"
              name="imgUrl"
              class="form-control"
              placeholder="Confirm imgUrl"
              value="<%= typeof password2 != 'undefined' ? imgUrl : '' %>"
            />
          </div>
          <button type="submit" class="btn btn-success btn-block">
            Add new
          </button>
        </form>
       
      </div>
    </div>
  </div>